{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "arg None",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/${workspaceFolderBasename}/bin/Debug/net9.0-windows/${workspaceFolderBasename}.dll",
            "args": [
                "--show"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
        },
